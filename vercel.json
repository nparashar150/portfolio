{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://nparashar150.val.run/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ]
}
